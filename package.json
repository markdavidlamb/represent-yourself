{
  "name": "counsel",
  "version": "1.0.0",
  "description": "AI-powered legal document assistant with Google integration",
  "author": {
    "name": "Counsel Team",
    "email": "team@counsel.app"
  },
  "homepage": "https://github.com/counsel/counsel",
  "repository": {
    "type": "git",
    "url": "https://github.com/counsel/counsel.git"
  },
  "license": "MIT",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:electron": "npm run build && npm run electron:compile",
    "start": "next start",
    "lint": "next lint",
    "cli": "ts-node src/cli/index.ts",
    "electron:compile": "tsc -p electron/tsconfig.json",
    "electron": "npm run electron:compile && electron .",
    "electron:dev": "npm run electron:compile && concurrently \"npm run dev\" \"wait-on http://localhost:3000 && NODE_ENV=development electron .\"",
    "electron:build": "npm run build:electron && electron-builder",
    "electron:build:mac": "npm run build:electron && electron-builder --mac",
    "electron:build:win": "npm run build:electron && electron-builder --win",
    "electron:build:linux": "npm run build:electron && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "bin": {
    "counsel": "./src/cli/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.0.7",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "commander": "^12.0.0",
    "framer-motion": "^12.23.26",
    "googleapis": "^131.0.0",
    "lucide-react": "^0.312.0",
    "next": "14.1.0",
    "ollama": "^0.5.0",
    "ora": "^5.4.1",
    "pdfjs-dist": "^5.4.449",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  }
}
